<script>
     $('.sidenav').sidenav();
   $.get('/api/reservation').then(data=>{
      data.forEach(data=>{
   let table = $(
   	`   <tr>
   		<td>${data.firstname}</td>
   		<td>${data.lastname}</td>

         <td>${data.email}</td>
   		<td>${data.phone}</td>
   
   		<td>${data.arrival}</td>
   		<td>${data.departure}</td>

         <td>${data.roomtype}</td>
